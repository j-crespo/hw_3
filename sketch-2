function setup() {
  createCanvas(500, 500);
  background(255);
}

function draw() {
  
// ellipseMode(Radius);
noFill();
// ellipse (250, 250, 480, 480);
// ellipse (250, 250, 20, 20);


for (var i = 20; i <= 480; i = i + 10){
  var sX = 250;
  var sY = 250;
  var eX = i;
  var eY = i;
  ellipse (sX, sY, eX, eY);
  }
}
