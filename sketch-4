function setup() {
  createCanvas(500, 500);
  background(255);
}

function draw() {
// line (20, 20, 480, 20);
// line (20, 480, 480, 480);


for (var i = 20; i <= 480; i = i + 10) {
    var sX = 20;
    var sY = i;
    var eX = 480;
    var eY = i;
    line (sX, sY, eX, eY);
  }
noStroke();
triangle (0, 270, 0, 0, 270, 0);
triangle (0, 230, 0, 500, 270, 500);
triangle (230, 0, 500, 0, 500, 270);
triangle (230, 500, 500, 230, 500, 500);
}
